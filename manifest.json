{
  "manifest_version": 2,

  "name": "CSV export for Clubhouse.io Reports",
  "short_name": "CSV Clubhouse.io",
  "description": "CSV export for Clubhouse.io reports",
  "version": "0.1.0",
  "browser_action": {
    "default_title": "CSV Clubhouse.io"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "icons": {
    "16": "images/csv-clubhouse-16.png",
    "48": "images/csv-clubhouse-48.png",
    "128": "images/csv-clubhouse-128.png"
  },

  "permissions": [
    "activeTab"
  ]

}
